	"tokenColors": [
		{
			"scope": "none",
			"settings": {
				"foreground": "#4C4C4C"
			}
		},
		{
			"scope": "comment, punctuation.definition.comment",
			"settings": {
				"foreground": "#a3a3a3"
			}
		},
		{
			"scope": "invalid.illegal",
			"settings": {
				"foreground": "#0A0A0A"
			}
		},
		{
			"scope": "invalid.broken",
			"settings": {
				"foreground": "#FFFFFF"
			}
		},
		{
			"scope": "invalid.deprecated",
			"settings": {
				"foreground": "#0A0A0A"
			}
		},
		{
			"scope": "invalid.unimplemented",
			"settings": {
				"foreground": "#0A0A0A"
			}
		},
		{
			"scope": "token.info-token",
			"settings": {
				"foreground": "#316BCD"
			}
		},
		{
			"scope": "token.warn-token",
			"settings": {
				"foreground": "#CD9731"
			}
		},
		{
			"scope": "token.error-token",
